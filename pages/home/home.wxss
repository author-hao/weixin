/* 首页 */
.home {
  width: 100%;
  background: #f1f1f1;
}
/* 头部欢迎 */
.home .home-title {
  width: 100%;
  font-size: 30rpx;
  padding: 20rpx;
  background: #fff;
}
/* mian主体 */
.home .home-warpper {
    width: 100%;
    overflow-y: scroll;
}
.home .home-content {
  width: 100%;
  height: 100%;
}
.home .home-content .home-swiper {
  width: 100%;
}
/* 轮播 */
.home .home-content .home-swiper .home-item {
  width: 100%;
  background: #fff;
}
.home .home-content .home-swiper .home-item .slide-image {
  width: 100%;
}
/* nav */
.home .home-content .home-icon {
  width: 100%;
  margin-top: 15rpx;
  padding: 15rpx 0;
  background: #fff;
}
.home .home-content .home-icon .home-group {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  background: #fff;
}
.home .home-content .home-icon .home-group .home-block {
  width: 20%;
  display: block;
  text-align: center;
}
.home .home-content .home-icon .home-group .icon {
  display: block;
  text-align: center;
  margin-top: 20rpx;
}
.home .home-content .home-icon .home-group text {
  display: block;
}
.home .home-theme {
  margin-top: 15rpx;
}
.home .home-theme .weui-panel .weui-panel__hd {
  color: #333;
  font-size: 40rpx;
}